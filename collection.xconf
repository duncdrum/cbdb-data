            <create qname="no:c_assoc_type_id" type="xs:integer"/>
            <create qname="no:c_status_type_code" type="xs:integer"/>
            <create qname="no:c_assoc_id" type="xs:integer"/>
            <create qname="no:c_status_code" type="xs:integer"/>
            <create qname="no:c_entry_code" type="xs:integer"/>
            <create qname="no:c_entry_type" type="xs:integer"/>
            <create qname="no:c_event_code" type="xs:integer"/>
