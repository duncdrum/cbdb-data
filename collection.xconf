            <create qname="no:c_office_id" type="xs:integer"/>
            <create qname="no:c_office_type_id" type="xs:integer"/>
            <create qname="no:c_office_tree_id" type="xs:integer"/>
            <create qname="no:c_posting_id" type="xs:integer"/>
            <create qname="no:c_office_chn" type="xs:string"/>
            <create qname="no:c_office_trans" type="xs:string"/>
            <create qname="no:c_office_pinyin" type="xs:string"/>
            <create qname="no:c_office_type_node_id" type="xs:integer"/>
            <create qname="no:c_office_category_id" type="xs:integer"/>
            <create qname="no:c_assume_office_code" type="xs:integer"/>
            <create qname="no:c_appt_type_code" type="xs:integer"/>
