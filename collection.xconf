            <!--  text related items      -->
            <create qname="no:c_source" type="xs:integer"/>
            <create qname="no:c_pages" type="xs:string"/><!--not all c_pages can be cast as integer -->
            <create qname="no:c_textid" type="xs:integer"/>
            <create qname="no:c_title" type="xs:string"/>
            <create qname="no:c_title_chn" type="xs:string"/>
            <create qname="no:c_sortorder" type="xs:integer"/>
            <create qname="no:c_publisher" type="xs:string"/>
